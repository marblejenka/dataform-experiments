config { 
  type: "table",
}
js {
  var { model_dataset } = require("includes/bqml_tutorial/constants");
}

#standardSQL
SELECT
  *
FROM
  ML.EXPLAIN_PREDICT(MODEL ${ref("penguins_model")}},
    (
    SELECT
      *
    FROM
      ${ref("penguins")}
    WHERE
      body_mass_g IS NOT NULL
      AND island = "Biscoe"),
    STRUCT(3 as top_k_features))
