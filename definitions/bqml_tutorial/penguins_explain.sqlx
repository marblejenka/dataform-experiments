config { 
  type: "table",
}

#standardSQL
SELECT
  ${constants.execution_time} as execution_time,
  *
FROM
  ML.EXPLAIN_PREDICT(MODEL ${penguins_model_simple_name}),
    (
    SELECT
      *
    FROM
      ${ref("penguins")}
    WHERE
      body_mass_g IS NOT NULL
      AND island = "Biscoe"),
    STRUCT(3 as top_k_features))
